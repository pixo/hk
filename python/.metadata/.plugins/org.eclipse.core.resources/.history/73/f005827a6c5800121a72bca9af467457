import pipeline.core as core
import pipeline.utils as utils


db=utils.dataBase.getDataBase()
project="testing"
sequence="001"
shot="001"

for seq in range(1,3):
    sqname="sq%03d" % seq
    core.sequence.createSequence(project, sqname, "ceci est une sequence", db)
    for sh in range(1,10):
        shname="%s_%03d"% (sqname,sh)
        core.shot.createShot(project, sqname, shname, "ceci est le shot %03d" % sh, 1, 100, db)

