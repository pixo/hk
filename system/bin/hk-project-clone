#!/usr/bin/env python
import sys, os
from PySide import QtGui
import badass.apps as apps

if __name__ == '__main__':
	if os.getenv("HK_PROJECT") == "ADMIN":
	    app = QtGui.QApplication ( sys.argv )    
	    main = apps.UiProjectCloner ()
	    main.show()
	    app.exec_()
	    sys.exit()
	else:
		print "You should run hk-admin"


